{
  "common": {
    "pass_metadata_only": false,
    "management_port": 8080,
    "stats_period": {
      "secs": 60,
      "nanos": 0
    },
    "job_writer_cache_max_capacity": 1000,
    "job_writer_cache_ttl": {
      "secs": 60,
      "nanos": 0
    },
    "job_eviction_ttl": {
      "secs": 60,
      "nanos": 0
    }
  },
  "in_stream": {
    "url": "router+bind:ipc:///tmp/${SOCKET_PATH_IN:-undefined}",
    "options": {
      "receive_timeout": {
        "secs": 1,
        "nanos": 0
      },
      "receive_hwm": 1000,
      "topic_prefix_spec": {
        "source_id": "source_id"
      },
      "source_cache_size": 1000,
      "inflight_ops": 100,
      "fix_ipc_permissions": 511
    }
  },
  "out_stream": null,
  "storage": {
    "rocksdb": {
      "path": "${DB_PATH:-/tmp/rocksdb}",
      "data_expiration_ttl": {
        "secs": 60,
        "nanos": 0
      }
    }
  }
}